{
  "name": "angular2-soundcloud",
  "version": "0.0.1",
  "description": "A basic soundcloud client view API implemented in Angular2, TypeScript and Webpack",
  "author": {
    "name": "Jay Kan",
    "email": "leicasper@gmail.com"
  },
  "scripts": {
    "clean": "del-cli ./public",
    "build:ngc": "ngc",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "build:prod": "cross-env NODE_ENV=production webpack --display-chunks",
    "build": "run-s build:ngc build:prod",
    "test:client": "cross-env NODE_ENV=test karma start --single-run --coverage",
    "test": "cross-env NODE_ENV=test karma start",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
    "production": "npm run clean && npm run build:prod && npm run server",
    "development": "npm run build:dev",
    "start": "npm run development"
  },
  "license": "MIT",
  "engines": {
    "node": ">=5.11",
    "npm": ">3.x"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "helmet": "^2.1.3",
    "serve-favicon": "^2.3.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@angular/common": "~2.4.2",
    "@angular/compiler": "~2.4.2",
    "@angular/compiler-cli": "~2.4.2",
    "@angular/core": "~2.4.2",
    "@angular/forms": "~2.4.2",
    "@angular/http": "~2.4.2",
    "@angular/platform-browser": "~2.4.2",
    "@angular/platform-browser-dynamic": "~2.4.2",
    "@angular/router": "~3.4.2",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@types/jasmine": "~2.5.35",
    "@types/jquery": "^2.0.34",
    "@types/node": "~6.0.45",
    "autoprefixer": "~6.3.6",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "~2.4.1",
    "cross-env": "^3.1.3",
    "css-loader": "~0.23.1",
    "del-cli": "^0.2.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "~2.22.0",
    "ie-shim": "^0.1.0",
    "immutable": "^3.8.1",
    "istanbul-instrumenter-loader": "~0.2.0",
    "jasmine-core": "~2.5.0",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "morgan": "~1.7.0",
    "node-sass": "~3.8.0",
    "nodemon": "~1.9.2",
    "npm-run-all": "^3.1.1",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "rxjs": "~5.0.3",
    "sass-loader": "~4.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "~0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.2",
    "ts-loader": "~0.8.2",
    "typescript": "~2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.5",
    "webpack-md5-hash": "~0.0.5",
